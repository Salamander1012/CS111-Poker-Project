public class Poker {
	public static void main(String[] args) {
		Hand straight = new Hand();
		
		straight.addCard(new Card(1, 0));
		straight.addCard(new Card(2, 0));
		straight.addCard(new Card(3, 0));
		straight.addCard(new Card(4, 0));
		straight.addCard(new Card(5, 0));
		
		straight.numPairs();
	}
}